Algoritmo Ejemplo
	Definir nombre Como Caracter
	Definir  nota1, nota2, nota3, trabajo, examen, promedio_notas, trabajo_notas, examen_notas Como Real
	Definir i, n Como Entero
	Escribir Sin Saltar "Nombre: "
	Leer nombre
	Escribir Sin Saltar "Nota 1: "
	Leer nota1
	Escribir Sin Saltar "Nota 2: "
	Leer nota2
	Escribir Sin Saltar "Nota 3: "
	Leer nota3
	Escribir Sin Saltar "Trabajo final: "
	Leer trabajo
	Escribir Sin Saltar "Examen final: "
	Leer examen
	promedio_notas <- ((nota1 + nota2 + nota3) / 3) * 0.55
	trabajo_notas <- trabajo * 0.15
	examen_notas <- examen * 0.3
	total <- promedio_notas + trabajo_notas + examen_notas
	Escribir "La nota final de: ",nombre," es: ",total
	
FinAlgoritmo
